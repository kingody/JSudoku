<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="gr.auth.csd.sudoku.gui.Settings">
  <grid id="27dc6" binding="panel" layout-manager="GridBagLayout">
    <constraints>
      <xy x="30" y="20" width="490" height="400"/>
    </constraints>
    <properties>
      <minimumSize width="300" height="300"/>
      <preferredSize width="300" height="300"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="d33ff" class="javax.swing.JLabel" binding="title">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag top="0" left="0" bottom="20" right="0" weightx="0.0" weighty="0.0" ipadx="10" ipady="10"/>
        </constraints>
        <properties>
          <font size="18"/>
          <text value="Settings"/>
        </properties>
      </component>
      <grid id="bd266" binding="settingsPanel" layout-manager="GridBagLayout">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="da247" class="javax.swing.JCheckBox" binding="wordokuCheckBox">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="0" left="0" bottom="10" right="0" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <enabled value="true"/>
              <horizontalAlignment value="10"/>
              <selected value="false"/>
              <text value="Wordoku"/>
            </properties>
          </component>
          <component id="fd275" class="javax.swing.JLabel" binding="languageLabel">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="10" left="0" bottom="5" right="0" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <font name="Bahnschrift" size="15" style="0"/>
              <text value="Select Language"/>
            </properties>
          </component>
          <grid id="d7ec6" binding="languages" layout-manager="FlowLayout" hgap="10" vgap="20" flow-align="1">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="bd5aa" class="javax.swing.JRadioButton" binding="englishButton">
                <constraints/>
                <properties>
                  <text value="English"/>
                </properties>
              </component>
              <component id="bde93" class="javax.swing.JRadioButton" binding="greekButton">
                <constraints/>
                <properties>
                  <text value="Greek"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <component id="d18c9" class="javax.swing.JButton" binding="closeButton">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <enabled value="true"/>
          <text value="OK"/>
        </properties>
      </component>
    </children>
  </grid>
  <buttonGroups>
    <group name="buttonGroup">
      <member id="bd5aa"/>
      <member id="bde93"/>
    </group>
  </buttonGroups>
</form>
